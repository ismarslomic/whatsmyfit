{
  "name": "whatsmyfit",
  "version": "1.0.0",
  "description": "Whats my fit APIs",
  "main": "index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "postinstall": "yarn compile",
    "start": "cd build && node main",
    "compile": "node build.js && backpack build",
    "dev": "node build.js && backpack dev | pino -o 'short'",
    "test": "mocha --exit",
    "test:coverage": "yarn compile && nyc yarn test && cat ./coverage/lcov.info | codacy-coverage --token $CODACY_PROJECT_TOKEN && rimraf ./coverage",
    "check:nsp": "nsp check --reporter table",
    "pretest": "tslint --project . -t codeFrame"
  },
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0",
    "yarn": "1.7.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "pino": "^4.17.3",
    "source-map-support": "^0.5.6",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.2",
    "@types/pino": "^4.16.0",
    "awesome-typescript-loader": "^4.0.0",
    "backpack-core": "^0.7.0",
    "chai": "^4.1.2",
    "codacy-coverage": "^3.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nsp": "^3.2.1",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "supertest": "^3.1.0",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "lcovonly"
    ],
    "include": [
      "server/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "node_modules/"
    ],
    "report-dir": "./coverage",
    "temp-directory": "./coverage/.nyc_output"
  },
  "author": "Ismar Slomic <ismar@slomic.no>"
}
